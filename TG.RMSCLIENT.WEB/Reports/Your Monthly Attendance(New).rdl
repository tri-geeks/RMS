    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>MonthlyAttd</DataSourceName>
        <CommandText>SELECT        CAST(DATENAME(yy, ATTEN_DATEIN) AS INT) AS NYear
FROM            PAY_TRANS_WORK_ATTENDANCE AS T2
WHERE        (DATEDIFF(MONTH, ATTEN_DATEIN, GETDATE()) &lt;= 2)
GROUP BY DATENAME(yy, ATTEN_DATEIN)
ORDER BY DATENAME(yy, ATTEN_DATEIN) DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="NYear">
          <DataField>NYear</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Present">
      <Query>
        <DataSourceName>MonthlyAttd</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@EmpCode">
            <Value>=Parameters!EmpCode.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@MonthName">
            <Value>=Parameters!MonthName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@NYear">
            <Value>=Parameters!NYear.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        COUNT(T2.ATTEN_STATUS) AS P
FROM            HRS_EMPLOYEE AS T1 INNER JOIN
                         PAY_TRANS_WORK_ATTENDANCE AS T2 ON T1.EMP_CARD_NO = T2.EMP_CARD_NO
WHERE        (T1.EMP_CARD_NO = @EmpCode) AND (DATENAME(mm, T2.ATTEN_DATEIN) = @MonthName) AND (YEAR(T2.ATTEN_DATEIN) = @NYear) AND 
                         (T2.ATTEN_STATUS = 'P')
GROUP BY T2.ATTEN_STATUS</CommandText>
      </Query>
      <Fields>
        <Field Name="P">
          <DataField>P</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
