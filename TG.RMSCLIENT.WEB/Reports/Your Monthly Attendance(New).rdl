    <DataSet Name="DataSet4">
      <Query>
        <DataSourceName>MonthlyAttd</DataSourceName>
        <CommandText>SELECT DISTINCT DATEPART(m, ATTEN_DATEIN) AS ID, CAST(DATENAME(month, DATEADD(month, MONTH(ATTEN_DATEIN), 0) - 1) AS VARCHAR(20)) AS MName
FROM            PAY_TRANS_WORK_ATTENDANCE AS T2
WHERE        (DATEDIFF(MONTH, ATTEN_DATEIN, GETDATE()) &lt;= 1)
GROUP BY DATENAME(mm, ATTEN_DATEIN), DATEPART(m, ATTEN_DATEIN)
ORDER BY ID</CommandText>
      </Query>
      <Fields>
        <Field Name="MName">
          <DataField>MName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ID">
          <DataField>ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
