<!-- Start Reservation section -->
<section id="mu-reservation">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="mu-reservation-area">
                    <div class="mu-title">
                        <span class="mu-subtitle">@TRI_RMS.Language.Make_A</span>
                        <h2>@TRI_RMS.Language.Reservation</h2>
                        <i class="fa fa-spoon"></i>
                        <span class="mu-title-bar"></span>
                    </div>
                    <div class="mu-reservation-content">


                        <script src="~/JavascriptFile/PageScript/Booking.js"></script>

                        <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit. Ratione quidem autem iusto, perspiciatis, amet, quaerat blanditiis ducimus eius recusandae nisi aut totam alias consectetur et.</p>
                        <form class="mu-reservation-form" id="formR">
                            <div class="row">
                                <input type="hidden" name="BookingID" id="BookingID" />
                                <input type="hidden" name="InvoiceID" id="InvoiceID" />
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <input type="text" class="form-control" name="BookingDate" id="datepicker" placeholder="Date" required>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        @*<input type="text" class="form-control" placeholder="Reservation Type" name="ReservationTypeID">*@
                                        <select class="form-control" id="ReservationTypeID" name="ReservationTypeID" required>
                                            <option value="0">--Select User--</option>
                                            <option value="1">User-1</option>
                                            <option value="2">User-2</option>
                                            <option value="3">User-3</option>

                                        </select>
                                    </div>
                                </div>


                                <div class="col-md-6">
                                    <div class="form-group">
                                        <input type="text" class="form-control" placeholder="StartTime" name="StartTime" id="StartTime" readonly="readonly" required>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <input type="text" class="form-control" placeholder="EndTime" name="EndTime" id="EndTime" readonly="readonly" required>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <input type="text" class="form-control" placeholder="Booking Qty" name="BookingQty" id="BookingQty" required>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <input type="text" class="form-control" placeholder="Full Name" name="Name" id="Name" required>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <input type="email" class="form-control" placeholder="Email" name="Email" id="Email" required>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <input type="text" class="form-control" placeholder="Contact" name="Contact" id="Contact" required>
                                    </div>
                                </div>

                                <div class="col-md-12">
                                    <div class="form-group">
                                        <textarea class="form-control" cols="30" rows="10" placeholder="Your Message" name="Remark" id="Remark"></textarea>
                                    </div>
                                </div>

                                <button type="submit" class="mu-readmore-btn">@TRI_RMS.Language.Make_Reservation</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
