<section id="mu-contact" style="z-index:900">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="mu-contact-area">

                    <div class="mu-title">
                        <span class="mu-subtitle">@TRI_RMS.Language.Get_In_Touch</span>
                        <h2>@TRI_RMS.Language.Contact_Us</h2>
                        <i class="fa fa-spoon"></i>
                        <span class="mu-title-bar"></span>
                    </div>
                    <div class="mu-contact-content">
                        <div class="row">
                            @*Contact with Rating*@
                            <form class="" id="fContact">
                                <div class="col-md-6">
                                    <div class="mu-contact-left mu-contact-form">
                                        @*<form class="mu-contact-form" id="fContact">*@
                                        <div class="form-group mu-contact-form">
                                            <label for="name">@TRI_RMS.Language.Your_Name</label>
                                            <input type="text" class="form-control" id="Name" name="Name" placeholder="Name" required>
                                        </div>
                                        <div class="form-group mu-contact-form">
                                            <label for="email">@TRI_RMS.Language.Email_Address</label>
                                            <input type="email" class="form-control" id="EmailId" name="EmailId" placeholder="Email" required>
                                        </div>
                                        <div class="form-group mu-contact-form">
                                            <label for="email">@TRI_RMS.Language.Phone</label>
                                            <input type="text" class="form-control" id="Phone" name="Phone" placeholder="Phone">
                                        </div>

                                        <div class="form-group mu-contact-form">
                                            <label for="subject">@TRI_RMS.Language.Subject</label>
                                            <input type="text" class="form-control" id="Subject" name="Subject" placeholder="Subject" required>
                                        </div>
                                        <div class="form-group mu-contact-form">
                                            <label for="message">@TRI_RMS.Language.Message</label>
                                            <textarea class="form-control" id="Message" name="Message" cols="30" rows="10" placeholder="Type Your Message"></textarea>
                                        </div>
                                        <button type="submit" class="mu-send-btn">@TRI_RMS.Language.Send_Message</button>

                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="mu-contact-right mu-contact-form">
                                        @*<div class="form-group mu-contact-form">
                                                <label for="message">Message</label>
                                                <textarea class="form-control" id="Message" name="Message" cols="30" rows="10" placeholder="Type Your Message"></textarea>
                                            </div>*@
                                        <style type="text/css">
                                            .class_checkbox {
                                                width: 20px;
                                                height: 20px;
                                                /*background-color: red;*/
                                                padding: 25px;
                                                background-image: url(/assets/img/menu/Star-blank.jpg);
                                                background-repeat: no-repeat;
                                            }

                                                .class_checkbox.checked {
                                                    /*background-color: green;*/
                                                    background-image: url(/assets/img/menu/Star-Shape.png);
                                                    background-repeat: no-repeat;
                                                }
                                        </style>
                                        <div class="mu-contact-widget">
                                            <h3>Rate Us</h3>
                                            <div class="rating" style="padding:10px">                                                                                             
                                                @Html.CheckBox("Star1", new { @id = "Star1", @class = "input_class_checkbox" })                                                
                                                @Html.CheckBox("Star2", new { @id = "Star2", @class = "input_class_checkbox" })                                                
                                                @Html.CheckBox("Star3", new { @id = "Star3", @class = "input_class_checkbox" })                                                
                                                @Html.CheckBox("Star4", new { @id = "Star4", @class = "input_class_checkbox" })                                                
                                                @Html.CheckBox("Star5", new { @id = "Star5", @class = "input_class_checkbox" })
                                            </div>
                                            <script type="text/javascript">
                                                $(function () {
                                                    $('.input_class_checkbox').each(function () {
                                                        $(this).hide().after('<div class="class_checkbox col-lg-2 col-md-2" />');

                                                    });

                                                    $('.class_checkbox').on('click', function () {
                                                        $(this).toggleClass('checked').prev().prop('checked', $(this).is('.checked'))
                                                    });
                                                })

                                            </script>
                                            <div class="mu-contact-widget">

                                            </div>
                                        </div>
                                    </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
